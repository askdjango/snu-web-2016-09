    "def base(num=None):\n",
    "    def deco(fn):\n",
    "        def inner(*args, **kwargs):\n",
    "            result = num*fn(*args, **kwargs)\n",
    "            return result\n",
    "        return inner\n",
    "    return deco\n",
    "\n",
    "@base(10)\n",
    "def sum(x, y):\n",
    "    return x + y\n",
    "\n",
    "print(sum(1, 2))"
