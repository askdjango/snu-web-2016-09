    "def base(base_num):\n",
    "    def set_decorator(fn):\n",
    "        def wrap(x,y):\n",
    "            return x + y + base_num\n",
    "        return wrap\n",
    "    return set_decorator\n",
    "\n",
    "@base(10)\n",
    "def sum(x,y):\n",
    "    return x + y\n",
    "print(sum(1,2))"
