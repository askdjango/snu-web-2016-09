    "def base(base_number):\n",
    "    def set_base(fn):\n",
    "        def wrap(x,y):\n",
    "            return x + y + base_number\n",
    "        return wrap\n",
    "    return set_base\n",
    "\n",
    "@base(10)\n",
    "def sum(x,y):\n",
    "    return x + y\n",
    "print(sum(1,2))\n",
    "\n",
